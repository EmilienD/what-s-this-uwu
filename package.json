{
  "name": "what-s-this-uwu",
  "description": "Every object-oriented Javascript developer's favourite game, guessing what \"this\" is in the current context.",
  "version": "0.1.0",
  "author": "Emilien Durieu <contact@emilienduri.eu>",
  "dependencies": {
    "classnames": "2.3.1",
    "final-form": "4.20.2",
    "gatsby": "^3.10.1",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-offline": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-source-filesystem": "3.10.0",
    "gatsby-transformer-json": "3.10.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "prop-types": "^15.7.2",
    "query-string": "7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "6.5.3",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "15.4.4"
  },
  "devDependencies": {
    "eslint": "7.31.0",
    "prettier": "2.3.2"
  },
  "keywords": [
    "javascript",
    "this"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "test:puzzles": "for PUZZLE in $(ls src/puzzles); do node src/test.js src/puzzles/$PUZZLE; done"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmilienD/what-s-this-uwu"
  },
  "bugs": {
    "url": "https://github.com/EmilienD/what-s-this-uwu/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
