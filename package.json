  "version": "0.1.0",
  "author": "Emilien Durieu <contact@emilienduri.eu>",
  "dependencies": {
    "classnames": "2.2.6",
    "final-form": "4.20.1",
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "2.3.18",
    "gatsby-transformer-json": "2.4.10",
    "gatsby-transformer-sharp": "^2.5.7",
    "prop-types": "^15.7.2",
    "query-string": "6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "6.5.0",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "12.2.1"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "keywords": [
    "javascript",
    "this"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "test:puzzles": "for PUZZLE in $(ls src/puzzles); do node src/test.js src/puzzles/$PUZZLE; done"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmilienD/what-s-this-uwu"
  },
  "bugs": {
    "url": "https://github.com/EmilienD/what-s-this-uwu/issues"
  }
